<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="using:TeamCapacityBalancing.ViewModels"
		xmlns:models="using:TeamCapacityBalancing.Models"
		Width="300"	Height="150"
		CanResize="False"
        x:Class="TeamCapacityBalancing.Views.DeleteLocalFilesWindow"
        Title="DeleteLocalFilesWindow"
		x:CompileBindings="True">

		<StackPanel Orientation="Vertical" VerticalAlignment="Center" HorizontalAlignment="Center">
		<TextBlock  Name="message"></TextBlock>
		<StackPanel Orientation="Horizontal">
			<Button Content="Yes" Margin="40 10 0 0" Click="Delete"/>
			<Button Content="No" Margin="40 10 0 0" Click="Close"/>
		</StackPanel>
		</StackPanel>
</Window>
